class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> m;
        for (int i=0;i<nums.size();i++){
            int diff = target - nums[i];
            if(m.count(diff)){
                return {i,m[diff]};
            }
            m.insert(pair<int,int>(nums[i],i));
        }
        return {};
    }
};
